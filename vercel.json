{
  "version": 2,
  "builds": [
    {
      "src": "bloodsmearimageanalysisproject/project/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "bloodsmearimageanalysisproject/project/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/register",
      "dest": "/bloodsmearimageanalysisproject/project/api/register.js"
    },
    {
      "src": "/api/login",
      "dest": "/bloodsmearimageanalysisproject/project/api/login.js"
    },
    {
      "src": "/api/test",
      "dest": "/bloodsmearimageanalysisproject/project/api/test.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/bloodsmearimageanalysisproject/project/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/bloodsmearimageanalysisproject/project/dist/$1"
    }
  ]
}
